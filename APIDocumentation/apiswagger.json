      operationId: "ricerca"
      produces:
      - "application/xml"
      - "application/json"
      responses:
        200:
          description: "successful operation"
          schema:
            $ref: "https://evening-stream-95878.herokuapp.com/searched"
        404:
          description: "Page not found"
      security:
      - api_key: []


